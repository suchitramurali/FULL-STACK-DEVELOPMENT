s=raw_input("enter a string")
n=0
p=0
k=0
j=0
for i in s:
	if i in "aeiouAEIOU":
		n=n+1
	elif i in "bcdfghjklmnpqrstvwxyBCDFGHJKLMNPQRSTVWXYZ":
		p=p+1
	elif i in" ":
		k=k+1
	elif i in "?":
		j=j+1
print ("the no of vowels is",n)
print ("the no of constants is",p)
print ("the no of words is",k+1)
print ("the no of ? is",j)
